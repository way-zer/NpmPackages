{
  "name": "simple-vite-vue-ssr",
  "version": "1.0.7",
  "description": "SSR util with vite and vue, with single entry and simple state",
  "author": "wayzer",
  "license": "MIT",
  "repository": "https://github.com/way-zer/NpmPackages/tree/master/packages/viteSSR",
  "type": "module",
  "keywords": [
    "vite",
    "plugin",
    "ssr",
    "vue"
  ],
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "vite build"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "typescript": "^4.8.4",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "vite": "^4.0.0",
    "vue": "^3.2.41"
  },
  "dependencies": {
    "simple-vite-vue-ssr": "^1.0.4"
  }
}
